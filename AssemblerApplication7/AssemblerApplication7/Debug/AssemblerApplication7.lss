                  //Setup ports
000000 ef0e       ldi r16,0xfe				;turn pnp transistors off
000001 b905       out portb,r16				
000002 ec07       ldi r16,0b11000111
000003 b904       out ddrb,r16
                 
000004 e003       ldi r16,0b00000011			;set 2 pins of portc to input
000005 b907       out ddrc, r16
000006 ef0f       ldi r16,0xff				;set all pins of port d to output
000007 b90a       out ddrd,r16
                  //defines the registers
                  .def ticks=r16				;defines r16 as ticks
                  .def secs=r17				;defines r17 as seconds
                  .def mins=r18				;defines r18 as minutes
                  .def hours=r19				;defines r19 as hours
                  //clears all registers
000008 2700       clr ticks					
000009 2711       clr secs
00000a 2722       clr mins
00000b 2733       clr hours
                 
                  //tick loop
                 tick:		
00000c 9b37              sbis pinc,7
00000d cffe              rjmp tick
00000e 9503              inc ticks
00000f 330c              cpi ticks, 60
000010 f501              brne display 
000011 2700              clr ticks
000012 c000      		rjmp inc_button
                 
                 inc_button:
000013 9840      		cbi portc,0
000014 991c      		sbic pinb,4
000015 c006      		rjmp set_button
000016 9a40      		sbi portc,0
000017 9523      		inc mins
000018 332c      		cpi mins,60
000019 f411      		brne set_button
00001a 2722      		clr mins
00001b c000      		rjmp set_button
                 
                 set_button:
00001c 9840      		cbi portc,0
00001d 991d      		sbic pinb,5
00001e c006      		rjmp tock1
00001f 9a40      		sbi portc,0
000020 9533      		inc hours
000021 3138      		cpi hours,24
000022 f411      		brne tock1
000023 2733      		clr hours
000024 c000      		rjmp tock1
                 		
                 tock1:        
000025 9513              inc secs
000026 331c              cpi secs, 60
000027 f449              brne display
000028 2711              clr secs
000029 9523              inc mins
00002a 332c              cpi mins, 60
00002b f429              brne display
00002c 2722              clr mins        
00002d 9533              inc hours
00002e 3138              cpi hours, 24
00002f f409              brne display
000030 2733              clr hours
                 
                  display:
000031 9510       com secs					;inverts the value in seconds
000032 b91b       out portd,secs				;ouputs the value in seconds to portd
000033 9510       com secs					;inverts seconds
000034 985e       cbi portd,6				;clears bit 6 in portd
000035 985f       cbi portd,7				;clears bit 7 in portd
000036 9828       cbi portb,0				;clears bit 0 in portb
000037 d02b       rcall delay_21k			;calls the delay subroutine
000038 9a28       sbi portb,0				;sets bit 0 in portb
                 
000039 2f43       mov r20,hours				;copies the value in hours to register 20
00003a d015       rcall get_segs				;call sthe subroutine get_segs
                 
00003b b95b       out portd,r21				;outputs the value in r21 to portd
00003c 9829       cbi portb,1				;clears bit 1 in portb
00003d d025       rcall delay_21k			;calls the delay subroutine
00003e 9a29       sbi portb,1				;sets bit 1 in portb
                 
00003f b96b       out portd,r22				;outputs the value in r22 to portd
000040 982a       cbi portb,2				;clears bit 2 in portb
000041 d021       rcall delay_21k			;calls the delay subroutine
000042 9a2a       sbi portb,2				;sets bit 2 in portb
                  
000043 2f42       mov r20, mins				;copies the value in minutes to r20
000044 d00b       rcall get_segs				;calls the get_segs subroutine
                 
000045 b95b       out portd,r21				;outputs the value in r21 to portd
000046 982e       cbi portb,6				;clears bit 6 in portb
000047 d01b       rcall delay_21k			;calls the delay subroutine
000048 9a2e       sbi portb,6				;sets bit 6 in portb
                   
000049 b96b       out portd,r22				;outputs the value in r22 to portd
00004a 982f       cbi portb,7				;clears bit 7 in portb
00004b d017       rcall delay_21k			;calls the delay subroutine
00004c 9a2f       sbi portb,7				;sets bit 7 in portb
                  
                   
                  tock:
00004d 9b37       sbis pinc,7				;checks to see if the pin when to 0
00004e cffe       rjmp tock					;jumps to tock
00004f cfbc       rjmp tick					;jumps to tick
                 
                 
                 
                 
                  /*get_segs_m takes an argument in scr and returns the most significant LED
                   segments (segs_h) and least significant LED segment (segs_l). The scr 
                   register is 0 to 59 for minutes 
                 
                   
                   		writes: clr, tens,zl, zh, segs_l, segs_h	*/
                 get_segs:
000050 2400      	clr r0				;clear tens register
                 div_m:
000051 304a      	cpi r20,10			;bigger than ten?		
000052 f018      	brlo done_div_m		;	no, done
000053 9403      	inc r0				;increment tens count
000054 504a      	subi r20,10			;subtract 10 from scr 
000055 cffb      	rjmp div_m          ;repeat until r20 is less than 10
                 done_div_m:
000056 e0f0      	ldi zh,high(seg_table<<1)	;load z register for indirect read
000057 efec      	ldi zl,low(seg_table<<1)
000058 0de0      	add zl,r0			;compute offset into the table
000059 f408      	brcc nc1_m			;carry required?
00005a 95f3      	inc zh				;carry
                 nc1_m:
00005b 9154      	lpm r21,z			;read segments from data table
00005c e0f0      	ldi zh,high(seg_table<<1)	;load z register for indirect read
00005d efec      	ldi zl,low(seg_table<<1)
00005e 0fe4      	add zl,r20			;compute offset into the table
00005f f408      	brcc nc2_m			;carry required?
000060 95f3      	inc zh				;carry
                 nc2_m:
000061 9164      	lpm r22,z			;read segments from data table
000062 9508      	ret	                ;return
                 
                 
                 // delay_21k simply calls delay 21 times
                 delay_21k:
000063 d015      	rcall delay
000064 d014      	rcall delay
000065 d013      	rcall delay
000066 d012      	rcall delay
000067 d011      	rcall delay
000068 d010      	rcall delay
000069 d00f      	rcall delay
00006a d00e      	rcall delay
00006b d00d      	rcall delay
00006c d00c      	rcall delay
00006d d00b      	rcall delay
00006e d00a      	rcall delay
00006f d009      	rcall delay
000070 d008      	rcall delay
000071 d007      	rcall delay
000072 d006      	rcall delay
000073 d005      	rcall delay
000074 d004      	rcall delay
000075 d003      	rcall delay
000076 d002      	rcall delay
000077 d001      	rcall delay
000078 9508      	ret
                 
                 
                 // delay takes 1003 cycles to execute
                 delay:               	;the label that identifies the subroutine to the assembler
000079 27ff          clr r31             ;initialize register 31 (this can be any unused register)
                 delay_loop:             ;a label for the brne instruction
00007a 95f3          inc r31             ;increment r31
00007b 3ff9          cpi r31,0xf9        ;compare r31 to the final value
00007c f7e9          brne delay_loop     ;jump to delay_loop if the count has not reached the final value
00007d 9508          ret                 ;return. The microcontroller 'remembers' where it came from
                 
                 
                 	// initialize LED segment table
                 seg_table:		
00007e eb09      	.dw 0xeb09				;1,0
00007f c185      	.dw 0xc185				;3,2
000080 5163      	.dw	0x5163				;5,4
000081 cb11      	.dw 0xcb11				;7,6
000082 4101      	.dw 0x4101				;9,8
000083 00ff      	.dw 0x00ff				;on,off
                 
                 
                 
                 
                 
