                 		rjmp delay_sequence1
                 		sbis pinb,5
                 		rjmp delay_sequence2
                 		sbis pinb,4				
                 		rjmp delay_sequence3
                 		cbi portc,0				;turns LED off
                 		rjmp start
                 
                 		delay_sequence1:
                 		sbi portc,1			;122 cycle delay loop
                 		rcall delay_122		
                 		cbi portc,1
                 		rcall delay_122
                 		sbi portc,0
                 		rjmp start
                 
