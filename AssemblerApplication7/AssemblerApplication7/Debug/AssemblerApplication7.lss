                 		sbi pinb,1			;82 cycle delay loop
                 	    rcall delay_82
                 		cbi portc,1
                 		rcall delay_82
                 		sbi portc,0
                 		rjmp start
                 
                 		//delay subroutines
                 		delay_122:
                 		clr r31
                 		delay_loop1:
                 		inc r31
                 		cpi r31,0x1e
                 		brne delay_loop1
                 		ret
                 
                 		delay_97:
                 		clr r31
                 		delay_loop2:
                 		inc r31
                 		cpi r31,0x18
                 		brne delay_loop2
                 		ret
                 
                 		delay_82:
                 		clr r31
                 		delay_loop3:
                 		inc r31
                 		cpi r31,0x16
                 		brne delay_loop3
                 		ret
                 


