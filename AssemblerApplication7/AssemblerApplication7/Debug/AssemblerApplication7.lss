                 	//setup portc
                 		ldi r16,0b01111100	;enable pullups, set speaker and led off
                 		out portc,r16
                 		ldi r16,0b00000011
                 		out ddrc,r16
                 
                 	//setup portd
                 		ldi r16,0b11111111	;set all pins as output, initialize high
                 		out portd,r16
                 		out ddrd,r16
                 
